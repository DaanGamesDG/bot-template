{
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"env": {
		"es2020": true,
		"node": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 11
	},
	"rules": {
		"no-var": 2,
		"@typescript-eslint/ban-ts-comment": 0,
		"no-fallthrough": 2,
		"no-unreachable": 2
	}
}
